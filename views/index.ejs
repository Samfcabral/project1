    <link rel="icon" href="../../favicon.ico">

    <title>Mi Casa</title>

    <link rel="stylesheet" href="/stylesheets/bootstrap.min.css">
    <!-- Custom Edits -->
    <link rel="stylesheet" href="stylesheets/main.css">

  </head>

  <body class="homey">

  <!-- Start of navigation -->
    <nav class="navbar navbar-default navbar-static-top" role="navigation">
    <div class="container">  
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/">Mi Casa</a>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
            <li class="active"><a href="/">Home</a></li>
            <% if (!currentUser)  {%> 
            <li><a href="/register">Register</a></li>
            <li><a href="/login">Login</a></li>
            <% }else{ %>
            <li><a href="/logout">Logout</a></li>
            <li><a href="/show">Watch List</a></li>
            <% }%>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </nav>
   <!-- End of Navigation --> 
<div class="cover-container">
    <div class="site-wrapper">

      <div class="site-wrapper-inner">

        

          <div class="inner cover">
            <h1 class="cover-heading">FIND YOUR NEXT HOME</h1>
            <p class="lead">Enter an address to calculate approximate cash flow for property.</p>
          </div>

          <div>
            <form method="GET" action="/search">
              <input type="text" name="address" placeholder="Street address">
              <input type="text" name="city" placeholder="City">
              <input type="text" name="state" placeholder="State">
              <input type="text" name="zip" placeholder="Zip Code">
              <button class="btn btn-lg btn-primary">SEARCH NOW</button>
            </form>

          </div>

        </div>

      </div>

    </div><!-- /container -->

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
